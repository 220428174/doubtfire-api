<%= render 'layouts/projects_list', :projects => @projects, :selected_project => nil %>
<%= render 'layouts/activity_stream', :projects => @projects %>
<div class="span4 widget">
	<h2>Progress</h2>
	<div class="widget-content" id="progress-widget">
		<% @projects.each do |project| %>
			<%= render 'dashboard/project_progress', :project => project %>
		<% end %>
	</div>
</div>